    Dim lngMin As Long                  'Å¬ƒCƒ“ƒfƒbƒNƒX
    Dim lngMax As Long                  'Å‘åƒCƒ“ƒfƒbƒNƒX
    Dim lngChkIndex As Long             'ŠY“–ƒCƒ“ƒfƒbƒNƒX
    Dim lngWorkId   As Long             '•W€‚h‚c

    On Error Resume Next
    
    '‰Šú‰»
    lngMin = 0
    lngMax = ID_GATE_MAX - 1
    lngChkIndex = -1

    'ŒŸõŠJn
    Do While lngMin <= lngMax
        lngIndex = lngMin
